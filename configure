: ${ac_max_here_lines=38}

# Identity of this package.
PACKAGE_NAME='cnqp'
PACKAGE_TARNAME='cnqp'
PACKAGE_VERSION='1.0'
PACKAGE_STRING='cnqp 1.0'
PACKAGE_BUGREPORT='yi.luo@alibaba-inc.com'

ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS APR APU APXS PHP CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT'
ac_subst_files=''

# Initialize some variables set by options.
ac_init_help=
